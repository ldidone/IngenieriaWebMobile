<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WappoMobile.Views.SeguimientoPage">
    <ContentPage.Content>
        <StackLayout Padding="30">
            <Label Text="Activar el seguimiento" FontSize="Large" TextColor="Black"></Label>
            <Switch x:Name="activarSeguimiento" VerticalOptions="Start" Toggled="activarSeguimiento_Toggled"></Switch>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>